!define cons
   !lambda head
     !lambda tail
       !lambda f
         !funcall !funcall f head tail;

!define car
  !lambda lis
    !funcall lis !lambda head
      !lambda tail
        head;

!define cdr
  !lambda lis
    !funcall lis !lambda head
      !lambda tail
        tail;

!define aList 
  !funcall !funcall cons 3 !funcall !funcall cons 2 !funcall !funcall cons 1 0;

!print !funcall car !funcall cdr aList