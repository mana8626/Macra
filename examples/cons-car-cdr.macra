!define cons
   !lambda head1
     !lambda tail1
       !lambda f
         !funcall !funcall f head1 tail1;

!define car
  !lambda lis1
    !funcall lis1 !lambda head2
      !lambda tail2
        head2;

!define cdr
  !lambda lis2
    !funcall lis2 !lambda head3
      !lambda tail3
        tail3;

!define aList 
  !funcall !funcall cons 3 !funcall !funcall cons 2 !funcall !funcall cons 1 0;

!print !funcall car !funcall cdr aList