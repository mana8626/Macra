#include prelude


#[ namespace name space : symbol -> namespace -> * = !define name !lambda msg space ]
#[ {a} : namespace -> * = a ]
#[ def id fn : * -> function -> namespace = if !equal id msg fn nil ]
#[ a; b : namespace -> namespace -> namespace = !lambda _ !if _ _ b a ]

namespace Bool
{ def "true" x, y => x
; def "false" x, y => y
};

!define true (Bool "true");
!define false (Bool "false");
!print (true 1 2);
!print (false 1 2)